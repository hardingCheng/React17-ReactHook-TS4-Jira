    "husky": ">=6",
    "json-server": "^0.16.3",
    "lint-staged": ">=10",
    "prettier": "2.3.2"
  },
  "husky": {
    "hooks": {
    }
  },
  "lint-staged": {
    "*.{js,css,md,jsx,ts,tsx}": "prettier --write"
